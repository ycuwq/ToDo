<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WeekView">
        <attr name="textSize" format="integer"/>
        <attr name="textColor" format="color" />
        <attr name="backgroundColor" format="color"/>
        <attr name="weekArray" format="string"/>
    </declare-styleable>
    <declare-styleable name="ExtraCalendarView">
        <!--是否显示农历-->
        <attr name="showLunar" format="boolean"/>
        <!--是否显示节假日-->
        <attr name="showHoliday" format="boolean" />
        <!--<attr name="backgroundColor" format="color"/>-->
        <attr name="textSizeTop" format="integer"/>
        <attr name="textColorTop" format="color" />
        <attr name="textSizeBottom" format="integer"/>
        <attr name="textColorTopBottom" format="color" />
    </declare-styleable>
</resources>